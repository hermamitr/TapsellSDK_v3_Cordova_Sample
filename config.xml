<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.tapsell.sample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TapsellSDKv3Sample</name>
    <description>
        A sample app which shows how tapsell sdk v3 works.
    </description>
    <author email="sinarezaei1991@gmail.com" href="http://tapsell.ir">
        Sina Rezaei
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="tapsell-v3-cordova-plugin" spec="^3.0.47" />
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="~4.4.0" />
</widget>
