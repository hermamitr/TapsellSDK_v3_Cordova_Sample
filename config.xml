<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.tapsell.sample.cordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Tapsell Cordova Sample</name>
    <description>
        A sample app which shows how tapsell cordova sdk v3 works.
    </description>
    <author email="info@tapsell.ir" href="http://tapsell.ir">
        Tapsell
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <engine name="android" spec="^6.4.0" />
    <engine name="ios" spec="~4.4.0" />
    <plugin name="tapsell-v3-cordova-plugin" spec="^3.1.13" />
</widget>
