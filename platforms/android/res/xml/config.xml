<?xml version='1.0' encoding='utf-8'?>
<widget id="ir.tapsell.sample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="TapsellCordovaInterface">
        <param name="android-package" value="ir.tapsell.sdk.TapsellCordovaInterface" />
    </feature>
    <name>TapsellSDKv3Sample</name>
    <description>
        A sample app which shows how tapsell sdk v3 works.
    </description>
    <author email="sinarezaei1991@gmail.com" href="http://tapsell.ir">
        Sina Rezaei
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
